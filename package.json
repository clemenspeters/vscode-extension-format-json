{
  "name": "format-json",
  "displayName": "vscode JSON formatter",
  "version": "1.0.0",
  "publisher": "clemens-peters",
  "description": "Convert file to file type json and format content.",
  "author": {
    "name": "Clemens Peters"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:extension.formatJSON"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.formatJSON",
        "title": "Format JSON"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^14.14.6",
    "@types/vscode": "^1.50.0",
    "mocha": "^2.3.3",
    "typescript": "^4.0.5"
  },
  "license": "ISC"
}
